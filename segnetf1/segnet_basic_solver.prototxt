net: "segnet_basic_train.prototxt"  		# Change this to the absolute path to your model file
test_initialization: true
test_iter: 25
test_interval: 100
base_lr: 1e-4
lr_policy: "fixed"
gamma: 0.9
stepsize: 100
display: 20
#momentum: 0.9
max_iter: 10000
weight_decay: 0.005
snapshot: 100
snapshot_prefix: "snapshots/gglr1e-5w0.005fixed"  	# Change this to the absolute path to where you wish to output solver snapshots
solver_mode: GPU 
#solver_type: ADAGRAD
