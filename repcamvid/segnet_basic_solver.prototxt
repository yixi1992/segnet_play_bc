net: "segnet_basic_train.prototxt"  		# Change this to the absolute path to your model file
test_initialization: true
test_iter: 100
test_interval: 100
base_lr: 0.1
lr_policy: "step"
gamma: 1.0
stepsize: 10000000
display: 20
momentum: 0.9
max_iter: 20000
weight_decay: 0.0005
snapshot: 100
snapshot_prefix: "snapshots/bs10lr0.1"  	# Change this to the absolute path to where you wish to output solver snapshots
solver_mode: GPU
